<PopulateCache name='Cache-Put-GCP-Logging-Token'>
  <Scope>Application</Scope>
  <Source>gcplogging.newtoken</Source>
  <CacheKey>
    <KeyFragment>gcplogging.token</KeyFragment>
  </CacheKey>
  <ExpirySettings>
    <TimeoutInSeconds ref='gcplogging.expires_in'>3540</TimeoutInSeconds> <!-- 3540 = 3600-60 = default ttl -->
  </ExpirySettings>
</PopulateCache>
